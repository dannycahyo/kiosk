# Cloudinary Configuration for Browser (Unsigned Upload)
# Get these from: https://cloudinary.com/console

# Your Cloudinary cloud name (found in Dashboard)
VITE_CLOUDINARY_CLOUD_NAME=your_cloud_name_here

# Unsigned upload preset name (create in Settings → Upload → Upload presets)
# IMPORTANT: Set "Signing Mode" to "Unsigned" when creating the preset
VITE_CLOUDINARY_UPLOAD_PRESET=photobooth_unsigned


# Application Base URL (for QR code generation)
VITE_BASE_URL=
